{
  "manifest_version": 3,
  "name": "Timer Alarm",
  "version": "1.0",
  "description": "A simple React Chrome Extension to set alarm that sent notifications",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "alarme.png",
      "48": "alarme.png",
      "128": "alarme.png"
    }
  },
  "permissions": [
    "storage","notifications","alarms"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
