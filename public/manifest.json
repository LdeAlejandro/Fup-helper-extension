{
  "manifest_version": 3,
  "name": "My React Extension",
  "version": "1.0",
  "description": "A simple React Chrome Extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage","notifications"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
